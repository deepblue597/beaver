
kafka {
        broker: "localhost:39092"
        input_topic: "CreditCard"
        consumer_group: "halfSpaceTree"
}


features {
        
        drop_features : { 
            
            minor
        } 

        generated_features: { 

            
            len_diff = new_length - old_length;
        }

}


algorithms { 

    model linear_model {

            type : linear_model
            name : logreg
            params : {
                lr = 1
            }
        }

    model linear_models {

            type : linear_model
            name : logreg
            params : {
                lr = 1
                preprocesor = linear_modelss
            }
        }


}

preproccessors { 

    model linear_modelss {

            type : linear_model
            name : logreg
            params : {
                lr = 1
            }
        }

    model linear_modelsss {

            type : linear_model
            name : logreg
            params : {
                lr = 1
            }
        }


}


optimizers { 

    model linear {

            type : linear_model
            name : logreg
            params : {
                lr = 1
            }
        }

    model linears {

            type : linear_model
            name : logreg
            params : {
                lr = 1
            }
        }


}


 
metrics { 

